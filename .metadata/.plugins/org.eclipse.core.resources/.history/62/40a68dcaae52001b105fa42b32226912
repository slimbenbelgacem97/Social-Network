import java.util.ArrayList;

public class Network {
	static ArrayList<Member> members;
	
	public static void createMember(String name) {
		Member m = new Member(name);
		members.add(m);
	}
	public static void main(String[] args) {
		createMember("Salata");
		for (Member m  : members ) {
			m.toString();
		}
	}
}
