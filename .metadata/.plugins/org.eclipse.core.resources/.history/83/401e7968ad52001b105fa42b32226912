import java.util.ArrayList;

public class Member {
	String name;
	ArrayList<Member> friends;
	//ArrayList<Post> wall;
	ArrayList<Member> requestsList;
	  
	
	public Member(String name) {
		
		this.name = name;
		
	}
	
	public void addRequest(Member member) {
		if(friends.indexOf(member)<0) {
			requestsList.add(this);
		}
		
	}
	
}
